Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"RunUniformMC", "[", 
    RowBox[{"dim_", ",", "nstep_", ",", "width_", ",", "c_"}], "]"}], ":=", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"nreject", "=", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"r", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "dim"}], "}"}]}], "]"}]}], ";", "\n", 
     "\t", 
     RowBox[{"rlist", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "nstep"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"rnew", "=", "r"}], ";", "\[IndentingNewLine]", 
        RowBox[{"Do", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"rnew", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "=", 
            RowBox[{"width", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"2", 
                RowBox[{"Random", "[", "]"}]}], "-", "1"}], ")"}]}]}], ";"}], 
          "\[IndentingNewLine]", ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", "dim"}], "}"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"Metropolis", "[", 
           RowBox[{"r", ",", "rnew", ",", "2"}], "]"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"r", "=", "rnew"}], ";"}], 
          RowBox[{"(*", 
           RowBox[{"use", " ", "metropolis", " ", "to", " ", "update"}], 
           "*)"}], "\[IndentingNewLine]", ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"nreject", "+=", "1"}], ";"}]}], 
         RowBox[{"(*", 
          RowBox[{
           RowBox[{"if", " ", "we", " ", 
            RowBox[{"don", "'"}], "t", " ", "update"}], ",", 
           RowBox[{"store", " ", "the", " ", "rejection", " ", "number"}]}], 
          "*)"}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"rlist", "[", 
          RowBox[{"[", "step", "]"}], "]"}], "=", 
         RowBox[{"Sqrt", "[", 
          RowBox[{"r", ".", "r"}], "]"}]}], ";"}], 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{
         "add", " ", "the", " ", "location", " ", "to", " ", "the", " ", 
          "list"}], ",", 
         RowBox[{
         "whether", " ", "or", " ", "not", " ", "we", " ", "changed", " ", 
          "it"}]}], "*)"}], "\[IndentingNewLine]", ",", 
       RowBox[{"{", 
        RowBox[{"step", ",", "1", ",", "nstep"}], "}"}]}], "]"}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{"iterate", " ", "over", " ", "nstep", " ", "tries"}], "*)"}], 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<Rejected \>\"", ",", 
       RowBox[{
        RowBox[{"100", 
         RowBox[{"nreject", "/", "nstep"}]}], "//", "N"}], ",", 
       "\"\<% of trials\>\""}], "]"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"norm", "=", 
      RowBox[{"NIntegrate", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"x", "^", 
          RowBox[{"(", 
           RowBox[{"dim", "-", "1"}], ")"}]}], 
         RowBox[{"Exp", "[", 
          RowBox[{
           RowBox[{"-", 
            RowBox[{"U1", "[", 
             RowBox[{"x", ",", "2"}], "]"}]}], "/", "kT"}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"x", ",", "0", ",", "\[Infinity]"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"th", "=", 
      RowBox[{"Plot", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"x", "^", 
          RowBox[{"(", 
           RowBox[{"dim", "-", "1"}], ")"}]}], 
         RowBox[{
          RowBox[{"Exp", "[", 
           RowBox[{
            RowBox[{"-", 
             RowBox[{"U1", "[", 
              RowBox[{"x", ",", "2"}], "]"}]}], "/", "kT"}], "]"}], "/", 
          "norm"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"x", ",", ".2", ",", "6.5"}], "}"}], ",", 
        RowBox[{"PlotRange", "\[Rule]", "All"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"sim", "=", 
      RowBox[{"ListPlot", "[", 
       RowBox[{"Distribution", "[", 
        RowBox[{"rlist", ",", "50", ",", ".2", ",", "7"}], "]"}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Show", "[", 
      RowBox[{"sim", ",", "th", ",", 
       RowBox[{"PlotRange", "\[Rule]", "All"}], ",", 
       RowBox[{"Frame", "\[Rule]", "True"}], ",", 
       RowBox[{"Axes", "\[Rule]", "False"}], ",", 
       RowBox[{"FrameLabel", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"\"\<|r|\>\"", ",", "\"\<P(r)\>\""}], "}"}]}], ",", 
       RowBox[{"PlotLabel", "\[Rule]", 
        RowBox[{"\"\<d = \>\"", "<>", 
         RowBox[{"ToString", "[", "dim", "]"}]}]}]}], "]"}]}], 
    "\[IndentingNewLine]", ")"}]}], ";"}]], "Input",
